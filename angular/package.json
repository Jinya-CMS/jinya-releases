{
  "name": "jinya-releases",
  "version": "3.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "extract-i18n": "ng extract-i18n --format json --output-path projects/admin/src/locale",
    "build": "npm run build:api && npm run build:admin",
    "build:api": "ng build api",
    "build:admin": "ng build admin",
    "build:dev": "npm run build:api:dev && npm run build:admin:dev",
    "build:api:dev": "ng build --configuration development api",
    "build:admin:dev": "ng build --configuration development admin",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "prettier --check projects/admin && ng lint",
    "format": "prettier --write projects/admin"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.0",
    "@angular/common": "19.2.0",
    "@angular/compiler": "19.2.0",
    "@angular/core": "19.2.0",
    "@angular/forms": "19.2.0",
    "@angular/localize": "19.2.0",
    "@angular/platform-browser": "19.2.0",
    "@angular/platform-browser-dynamic": "19.2.0",
    "@angular/router": "19.2.0",
    "@jinyacms/cosmo": "1.0.9",
    "@fontsource/albert-sans": "5.2.0",
    "@fontsource/oxygen-mono": "5.2.0",
    "@fontsource/urbanist": "5.2.0",
    "angular-oauth2-oidc": "19.0.0",
    "lucide-angular": "0.477.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.0",
    "@angular-eslint/builder": "19.1.0",
    "@angular/cli": "19.2.0",
    "@angular/compiler-cli": "19.2.0",
    "@eslint/js": "9.21.0",
    "angular-eslint": "19.1.0",
    "eslint": "9.21.0",
    "prettier": "3.5.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.25.0"
  }
}
